# Example environment variables for Vercel deployment
# Copy this file to .env for local development
# For Vercel, add these variables in the Vercel project settings

# Flask secret key (generate a secure random key for production)
SECRET_KEY=trademartkey123

# Database URL (Vercel will use a different database in production)
SQLALCHEMY_DATABASE_URI=sqlite:///trademart.db

# Set to False in production
DEBUG=False