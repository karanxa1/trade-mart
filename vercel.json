{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "exclude": [
    "trade_mart/static/**",
    "trade_mart/templates/**",
    "*.py",
    "*.md",
    "*.txt",
    "*.sqlite",
    "*.db",
    "*.log"
  ]
}